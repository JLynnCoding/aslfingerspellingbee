<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aslfingerspellingbee">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="aslfingerspellingbee.ASLFingerspellingBeeStatisticsActivity"></activity>
        <activity android:name="aslfingerspellingbee.SinkOrSignActivity" />
        <activity android:name="aslfingerspellingbee.IntermediateSpellingBeeActivity" />
        <activity android:name="aslfingerspellingbee.LetterQuizActivity" />
        <activity android:name="aslfingerspellingbee.BeginnerSpellingBeeActivity" />
        <activity android:name="aslfingerspellingbee.FlashcardActivity" />
        <activity android:name="aslfingerspellingbee.AlphabetChartActivity" />
        <activity android:name="aslfingerspellingbee.MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name="aslfingerspellingbee.Receiver" />

    </application>

</manifest>